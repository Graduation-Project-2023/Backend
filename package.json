{
  "name": "university-system-backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "lint": "eslint src/*.ts",
    "lint:f": "eslint src/**/**/*.ts --fix",
    "mocha": "mocha",
    "mocha:models": "mocha src/test/models/*.test.ts",
    "mocha:api": "mocha src/test/api/*.test.ts",
    "mocha:unit": "mocha --require  ts-node/register --no-config --timeout 20000000000 src/test/unit_tests/*.test.ts",
    "migrate": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:test": "dotenv -e .env.test npx prisma migrate dev",
    "seed": "npx prisma db seed",
    "test": "dotenv -e .env.test npm run mocha",
    "test:unit": "dotenv -e .env.test npm run mocha:unit",
    "test:models": "dotenv -e .env.test npm run mocha:models",
    "test:api": "dotenv -e .env.test npm run mocha:api"
  },
  "author": "Saif Helal, Mohamed Raafat",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@quixo3/prisma-session-store": "^3.1.10",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "chai": "^4.3.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prisma": "^4.5.0",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.6",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.34",
    "@types/superagent": "^4.1.16",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
